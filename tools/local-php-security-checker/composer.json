{
  "require-dev": {
    "tomchochola/local-php-security-checker": "dev-main"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/tomchochola/local-php-security-checker"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "tomchochola/local-php-security-checker": true
    },
    "sort-packages": true
  }
}
